<% if @success %>
  $('.delete').bind('ajax:success', function(){
    console.log($('.project_hours_logged_class')[0]);
    $('.project_hours_logged_class').html("<%= project_hours_logged(@project) %>");
    $(this).closest('tr').fadeOut();
  });
<% else %>
  $('.errors-div').replaceWith("<%= j render 'shared/errors', object: @time_log %>");
<% end %>
