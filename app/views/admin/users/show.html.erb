<h1>User</h1>
<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Email</th>
      <th>Role</th>
      <th>Address</th>
      <th>Cnic</th>
      <th>Phone Number</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
        <td><%= @user.name %></td>
        <td><%= @user.email %></td>
        <td><%= @user.role %></td>
        <td><%= @user.address %></td>
        <td><%= @user.cnic %></td>
        <td><%= @user.phone_number %></td>
  </tbody>
</table>

<br>

<%= link_to 'Edit', edit_admin_user_path, class: 'btn btn-info' %>
<%= link_to 'Back', admin_users_path, class: 'btn btn-info' %>
